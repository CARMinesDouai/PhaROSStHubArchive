{ #category : #testing }
BcLaserObstacleAvoiderTest >> setUp [
	super setUp.
	behavior safetyDistance: 0.5
]

{ #category : #testing }
BcLaserObstacleAvoiderTest >> testLaserIgnoresRearObstacles [
	| scanFrame |
	scanFrame := BcScanFrameMock
		angle_increment: 1
		angle_max: 3
		angle_min: -3
		ranges: #(0 0 10 10 10 0 0).
	behavior updateSpeedFromLaserScan: scanFrame.
	self assert: body leftSpeed equals: 0.5.
	self assert: body rightSpeed equals: 0.5
]

{ #category : #testing }
BcLaserObstacleAvoiderTest >> testLaserPerformance [
	| scanFrame |
	scanFrame := BcScanFrameMock
		angle_increment: 0.008
		angle_max: 2.094
		angle_min: -2.356
		ranges: self fullScanRanges.
	self should: [ behavior updateSpeedFromLaserScan: scanFrame ] notTakeMoreThan: 10 milliSeconds
]

{ #category : #testing }
BcLaserObstacleAvoiderTest >> testLaserWithFrontObstacle [
	| scanFrame |
	scanFrame := BcScanFrameMock
		angle_increment: 1
		angle_max: 1
		angle_min: -1
		ranges: #(10 0 10).
	behavior updateSpeedFromLaserScan: scanFrame.
	self assert: body leftSpeed equals: 0.5.
	self assert: body rightSpeed equals: 0
]

{ #category : #testing }
BcLaserObstacleAvoiderTest >> testLaserWithLeftObstacle [
	(0 to: 0.5 by: 0.1)
		do: [ :obstacleDistance | 
			self
				updateSpeedFromObstacleRanges:
					{10.
					10.
					obstacleDistance.}.
			self assertLeftSpeed: 0.5 rightSpeed: obstacleDistance ]
]

{ #category : #testing }
BcLaserObstacleAvoiderTest >> testLaserWithNoObstacle [
	| scanFrame |
	scanFrame := BcScanFrameMock
		angle_increment: 1
		angle_max: 1
		angle_min: -1
		ranges: #(10 10 10).
	behavior updateSpeedFromLaserScan: scanFrame.
	self assert: body leftSpeed equals: 0.5.
	self assert: body rightSpeed equals: 0.5
]

{ #category : #testing }
BcLaserObstacleAvoiderTest >> testLaserWithObstaclesBothOnTheLeftAndTheRight [
	(0 to: 0.4 by: 0.1)
		do: [ :obstacleDistance | 
			self
				updateSpeedFromObstacleRanges:
					{obstacleDistance.
					10.
					obstacleDistance}.
			self assertLeftSpeed: 0.5 rightSpeed: obstacleDistance ]
]

{ #category : #testing }
BcLaserObstacleAvoiderTest >> testLaserWithRightObstacle [
	(0 to: 0.5 by: 0.1) do: [ : obstacleDistance |
		self updateSpeedFromObstacleRanges: { obstacleDistance. 10. 10 }.
		self assertLeftSpeed: obstacleDistance rightSpeed: 0.5.
	]

]

