{ #category : #accessing }
ROBEmployee >> asDump [
	| emp |

	emp :=  ' {1} new charge: ''{2}''; name: ''{3}''; titles: ''{4}''; work: ''{5}'' ' format:  {self class name.  charge . name . titles . work }.
	
	ROBEmployee class compile:( ' {1} 
	^ {2}' format: {name copyReplaceAll: ' ' with: '' . emp}).
	
	^ ' ROBEmployee {1} ' format: {name copyReplaceAll: ' ' with: '' }
]

