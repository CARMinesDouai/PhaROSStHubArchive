{ #category : #accessing }
ROBEmployee >> email [
	^ ('.' join: (' ' split: name asLowercase)), '@mines-douai.fr'.
]

{ #category : #accessing }
ROBEmployee >> id [ 	
	^ self name copyReplaceAll: ' ' with: ''
]

{ #category : #accessing }
ROBEmployee >> image [
	| fileName |
	fileName :=  name copyReplaceAll: ' ' with: ''.
	fileName := fileName, 'Jpg'.
	^ ROBFileLibrary /fileName asSymbol.
]

